\mode<presentation>

  \usetheme[height=.55cm]{Rochester}
  \useinnertheme{circles}
  \definecolor{royalblue}{rgb}{0,0.13725490196078433,0.4}
  \definecolor{royalblueweb}{rgb}{0.25490196078431371,0.41176470588235292,0.88235294117647056}
  \definecolor{silver}{rgb}{0.75294117647058822,0.75294117647058822,0.75294117647058822}
  \definecolor{burntorange}{rgb}{0.8,0.3333333333333333,0}
  \setbeamercolor{palette primary}{fg=black,bg=royalblue}
  \setbeamercolor{titlelike}{parentpalette primary, fg=silver}
  \setbeamercolor{frametitle}{fg=silver}
  \setbeamercolor{itemize item}{fg=royalblue}
  \setbeamercolor{enumerate item}{fg=royalblue}

  %\usefonttheme{professionalfonts}
  \usefonttheme{professionalfonts}
  %font themes: default professionalfonts serif structurebold structureitalicserif structuresmallcapsserif
  %\setbeamertemplate{enumerate item}{\insertenumlabel.\ }

  \setbeamercovered{invisible} % or whatever (possibly just delete
                                % it)
  \setbeamercolor{math text}{fg=burntorange}
  \setbeamersize{text margin left=.5cm, text margin right=.5cm, text margin top=.1cm, text margin bottom=.5cm}
  \setbeamerfont{title}{family=\fontspec{Papyrus}}
  \setbeamerfont{title page}{family=\fontspec{Herculanum}}
  \setbeamerfont{frametitle}{family=\fontspec{Papyrus}}
\mode
<all>
